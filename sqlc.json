{
  "version": "2",
  "sql": [
    {
      "engine": "sqlite",
      "queries": "sql/",
      "schema": "migrations/",
      "gen": {
        "go": {
          "package": "store",
          "out": "internal/store",
          "emit_interface": true,
          "emit_json_tags": true,
          "json_tags_case_style": "snake"
        }
      }
    }
  ]
}
